<div class="card mb2">
  <div class="card-header bg-dark text-white">
    <h5><i class="fas fa-fire"></i> Most popular</h5>
  </div>
  <div class="card body p-2">
    <div class="row">
      {{#each sidebar.popular }}
        <div class="col-md-4 mb-2">
          <a href="/images/{{ uniqueId }}">
            <img class="w-100 h-100 img-thumbnail" src="/public/upload/{{ filename }}">
          </a>
        </div>
      {{/each}}
    </div>
  </div>
</div>